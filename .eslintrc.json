{
    "extends": [
        "airbnb-base",
        "airbnb-typescript/base"
    ],
    "rules": {
        "indent": ["error", 4],
        "import/extensions": ["off"]
    },
    "env": {
        "node": true
    },
    "parserOptions": {
        "project": "./tsconfig.json",
        "ecmaVersion": 2020
    },
    "ignorePatterns": [
        "prod/"
    ]
}
  